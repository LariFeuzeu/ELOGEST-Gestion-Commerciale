<Application x:Class="GES_COM_2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GES_COM_2"
             StartupUri="Views/FenetreloginView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="Explora">/StarLabsFileManager;component/Assets/Fonts/Explora.ttf#Explora</FontFamily>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Themes/LightTheme.xaml"/>

                <ResourceDictionary Source="Assets/Icons/Icons.xaml"/>

                <ResourceDictionary Source="Styles/MainStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
